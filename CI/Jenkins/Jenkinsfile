pipeline {
    agent any
    stages {
        stage('Collect Variables') {
            steps {
                echo "Version Choice is: ${GLPI_VERSION}"
                echo "Branch or Tag Choice is: ${BRANCH_TAG}"
                echo "SO Versions is: ${SO_VERSION}"               
            }
        }
        stage('Prepare Environment') {
            steps {
                script {
                    if (env.GLPI_VERSION == 'latest') {
                        echo 'latest'
                    } else {
                        echo 'Another Version'
                    }
                }
            }
        }
    }
}
